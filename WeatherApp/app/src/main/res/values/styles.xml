<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MainCardSubtitle" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/off_white</item>
        <item name="android:fontFamily">@font/montserrat700</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">top</item>
        <item name="android:letterSpacing">0.02</item>
    </style>
    <style name="MainCardSubtitleValue" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/montserrat700</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">25sp</item>
    </style>
    <style name="HourlyCard" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/montserrat700</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:lineSpacingExtra">3sp</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:textSize">14sp</item>
        
    </style>
    <style name="Toolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">25sp</item>
        <item name="fontFamily">@font/nunitosans700</item>
        <item name="android:lineSpacingExtra">6sp</item>
    </style>
    <style name="ActiveIndicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">#D6E3FF</item>
    </style>
</resources>